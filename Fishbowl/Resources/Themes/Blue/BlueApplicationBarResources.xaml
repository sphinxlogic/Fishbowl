<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <LinearGradientBrush x:Key="ApplicationBarBackground"
                 EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF00123B" Offset="1"/>
        <GradientStop Color="#FF041541"/>
        <GradientStop Color="#FF152B5F" Offset="0.583"/>
        <GradientStop Color="#FF19326F" Offset="0.435"/>
    </LinearGradientBrush>
    
    
